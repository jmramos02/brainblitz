<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="489.0" prefWidth="779.0" xmlns:fx="http://javafx.com/fxml" fx:controller="ph.edu.ue.rnd.brainblitz.AdminDashboardController">
  <children>
    <TabPane layoutX="14.0" layoutY="83.0" prefHeight="314.0" prefWidth="740.0" tabClosingPolicy="UNAVAILABLE">
      <tabs>
        <Tab text="Users">
          <content>
            <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
              <children>
                <TableView fx:id="userTable" layoutX="28.0" layoutY="38.0" prefHeight="200.0" prefWidth="705.0">
                  <columns>
                    <TableColumn prefWidth="100.0" text="ID" fx:id="id" />
                    <TableColumn prefWidth="150.0" text="Username" fx:id="username" />
                    <TableColumn minWidth="100.0" prefWidth="75.0" text="Login Status" fx:id="loginStatus" />
                    <TableColumn minWidth="100.0" prefWidth="75.0" text="Score" fx:id="score" />
                    <TableColumn minWidth="100.0" prefWidth="75.0" text="Role" fx:id="role" />
                  </columns>
                </TableView>
                <Button fx:id="addButton" layoutX="28.0" layoutY="254.0" mnemonicParsing="false" prefHeight="24.0" prefWidth="57.0" text="Add" onAction="#handleAddButton" />
                <Button fx:id="editButton" layoutX="101.0" layoutY="254.0" mnemonicParsing="false" prefHeight="24.0" prefWidth="57.0" text="Edit" />
                <Button fx:id="deleteButton" layoutX="174.0" layoutY="254.0" mnemonicParsing="false" prefHeight="24.0" prefWidth="57.0" text="Delete" onAction="#deleteUser" />
                <Label layoutX="28.0" layoutY="18.0" text="Search" />
                <TextField fx:id="searchTextField" layoutX="74.0" layoutY="15.0" prefWidth="200.0" />
                <Button fx:id="goButton" layoutX="284.0" layoutY="14.0" mnemonicParsing="false" text="GO &gt;" />
              </children>
            </AnchorPane>
          </content>
        </Tab>
        <Tab text="Questions">
          <content>
            <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
          </content>
        </Tab>
        <Tab text="Logs">
          <content>
            <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
          </content>
        </Tab>
      </tabs>
    </TabPane>
    <AnchorPane fx:id="header" layoutX="14.0" layoutY="14.0" prefHeight="64.0" prefWidth="740.0">
      <children>
        <Label layoutX="21.0" layoutY="14.0" text="T a g i s a n    n g    T a l i n o" textFill="WHITE">
          <font>
            <Font name="MyriadPro-Regular" size="24.0" />
          </font>
        </Label>
        <Label layoutX="27.0" layoutY="43.0" text="College of Computer Studies and System" textFill="WHITE" />
      </children>
    </AnchorPane>
  </children>
  <stylesheets>
    <URL value="@utils/login.css" />
  </stylesheets>
</AnchorPane>
